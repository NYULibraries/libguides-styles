<style type="text/css">
  nav#bobcat_tabs {
    display: block;
    margin-top: 20px;
  }
  #bobcat_tabs ul {
    list-style: none;
    margin-left: 0;
    padding-left: 0;
  }
  #bobcat_tabs ul > li {
    display: inline-block;
  }
  #bobcat_tabs ul > li a {
    border-radius: 10px;
    padding: 10px;
    background-color: whitesmoke;
  }
  #bobcat_tabs ul > li a {
    border-radius: 4px;
    padding: 10px;
    background-color: whitesmoke;
  }
  #bobcat_tabs ul > li a:hover {
    background-color: #e8e8e8;
  }
  #bobcat_tabs ul > li.current a {
    background-color: #e8e8e8;
  }

.arch_big_button a {
  background-color: #57068c;
  padding: 20px 0;
  text-align: center;
  border-radius: 10px;
  letter-spacing: 1px;
  margin-bottom: 20px;
  width: 100%;
  display: inline-block !important;
  font-size: 23px !important;
  letter-spacing: 1px !important;
  line-height: 25px !important;
}
.arch_big_button a, .arch_big_button a:hover, .arch_big_button a:active, .arch_big_button a:visited {
  color: white !important;
  text-decoration: none !important;
}
</style>
<style>

.s-lg-az-result-keywords {
display: none;
}

.s-lg-label-more-info button {
    font-size: 1.0em;
}    

.s-lg-content-more-info {
    font-size: 1.0em;
}

#s-lib-public-header-desc {
display: none;
}

</style>


	

